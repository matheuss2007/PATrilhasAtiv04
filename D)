			 string assistiu = (Console.ReadLine());
			  
			  
			  
			  

			    if (assinante == "sim" || assistiu == "sim" )
        {
			    	
           
		Console.WriteLine("Otimo, você pode assistir à nova temporada!");
		
			    	
        }
			    
			    
        else
        	
        {
        	
          
